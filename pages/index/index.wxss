/* pages/index/index.wxss */
navigator{
  width: 100%;
  height: 100%;
}
.id_banner{
  padding: 20rpx;
  height: 340rpx;
}
.id_nav{
  padding:0 40rpx 16rpx;
}
.id_nav_list {
  display: flex;
  overflow-x: auto;
  white-space: nowrap;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.id_nav_list navigator {
  width: 214rpx;
  height: 300rpx;
  margin-right: 14rpx;
  margin-bottom: 14rpx;
  display: inline-block;
}
.id_txt{
  color: #333;
  font-size: 34rpx;
  line-height: 100rpx;
  padding-left: 40rpx;
}